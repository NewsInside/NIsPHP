<window>
	<defaultcontrol>102</defaultcontrol>
	<controls>
		<control>
			<description>thumbnail dialog background</description>
			<type>image</type>
			<posX>160</posX>
			<posY>151</posY>
			<width>320</width>
			<height>190</height>
			<texture>dialog_background.png</texture>
		</control>

		<control>
			<type>label</type>
			<posX>175</posX>
			<posY>155</posY>
			<width>290</width>
			<align>left</align>
			<label>39</label>
			<textcolor>0x808080FF</textcolor>
			<font>arial_10</font>
		</control>

		<control>
			<description>name of file that is copied</description>
			<type>label</type>
			<id>100</id>
			<posX>180</posX>
			<posY>192</posY>
			<width>280</width>
			<align>center</align>
			<textcolor>0x808080FF</textcolor>
			<font>arial_10</font>
		</control>

		<control>
			<type>progress</type>
			<id>101</id>
			<posX>180</posX>
			<posY>230</posY>
			<width>280</width>
			<height>16</height>
			<textureleft>progress_left.png</textureleft>
			<textureright>progress_right.png</textureright>
			<texturemid>progress_mid.png</texturemid>
		</control>

		<control>
			<description>OK button</description>
			<type>button</type>
			<id>102</id>
			<posX>280</posX>
			<posY>290</posY>
			<width>100</width>
			<height>20</height>
			<texturefocus>select.png</texturefocus>
			<texturenofocus>blank.png</texturenofocus>
			<texturewidth>20</texturewidth>
			<textureheight>20</textureheight>
			<texturealign>left</texturealign>
			<label>15</label>
			<textcolor>0x80FFFFFF</textcolor>
			<selectedcolor>0x8000FF00</selectedcolor>
			<font>arial_10</font>
		</control>

		<control>
			<description>percent label</description>
			<type>label</type>
			<id>103</id>
			<align>center</align>
			<posX>180</posX>
			<posY>265</posY>
			<width>280</width>
			<textcolor>0x80FFFFFF</textcolor>
			<font>arial_10</font>
		</control>
	</controls>
</window>