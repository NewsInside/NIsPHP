<window>
	<defaultcontrol>101</defaultcontrol>
	<controls>
		<control>
			<description>thumbnail dialog background</description>
			<type>image</type>
			<posX>192</posX>
			<posY>192</posY>
			<width>256</width>
			<height>128</height>
			<texture>dialog_background.png</texture>
		</control>

		<control>
			<type>label</type>
			<posX>195</posX>
			<posY>192</posY>
			<width>250</width>
			<align>left</align>
			<label>14</label>
			<textcolor>0x808080FF</textcolor>
			<font>arial_10</font>
		</control>

		<control>
			<type>progress</type>
			<id>100</id>
			<posX>220</posX>
			<posY>245</posY>
			<width>200</width>
			<height>16</height>
			<textureleft>progress_left.png</textureleft>
			<textureright>progress_right.png</textureright>
			<texturemid>progress_mid.png</texturemid>
		</control>

		<control>
			<description>OK button</description>
			<type>button</type>
			<id>101</id>
			<posX>280</posX>
			<posY>280</posY>
			<width>100</width>
			<height>20</height>
			<texturefocus>select.png</texturefocus>
			<texturenofocus>blank.png</texturenofocus>
			<texturewidth>20</texturewidth>
			<textureheight>20</textureheight>
			<texturealign>left</texturealign>
			<label>15</label>
			<textcolor>0x80FFFFFF</textcolor>
			<selectedcolor>0x8000FF00</selectedcolor>
			<font>arial_10</font>
		</control>

		<control>
			<description>percent label</description>
			<type>label</type>
			<id>102</id>
			<align>center</align>
			<posX>192</posX>
			<posY>220</posY>
			<width>256</width>
			<textcolor>0x80FFFFFF</textcolor>
			<font>arial_10</font>
		</control>
	</controls>
</window>